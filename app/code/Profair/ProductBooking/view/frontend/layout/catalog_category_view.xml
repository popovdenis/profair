<?xml version="1.0"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <referenceBlock name="category.products.list">
                <block class="Profair\ProductBooking\Block\Product\BookProduct" name="profair.product.book"
                       template="Profair_ProductBooking::product/book.phtml" />
            </referenceBlock>
        </referenceContainer>

        <referenceBlock name="category.product.addto" remove="true" />
        <referenceBlock name="category.product.addto.comparo" remove="true" />
    </body>
</page>